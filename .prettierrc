{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5",
    "printWidth": 100,
    "jsxSingleQuote": false,
    "bracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "auto",
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": ["^react", "^next", "<THIRD_PARTY_MODULES>", "^@/", "^[./]"],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}
